	WEB_SEARCH : function(){
		return [
			{"text" : WORD_J_JS_DEFINE_001, "url"  : "http://www.google.com/search?hl=$(SOURCE)&q=%s"},
			{"text" : WORD_J_JS_DEFINE_002, "url"  : "http://translate.google.com/#$(SOURCE)/$(TARGET)/%s"},
			{"text" : WORD_J_JS_DEFINE_003, "url"  : "http://$(SOURCE).wikipedia.org/wiki/%s"}
		];
	},

